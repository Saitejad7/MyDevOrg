    public static String formattedDate(string formattedDate){
        String stringMonth    = formattedDate.substring(4,7);
        String day        = formattedDate.substring(8,10);
        String year       = formattedDate.substring(24,28);
        
        string month = monthValueByName(stringMonth);
        formattedDate = month +'/'+day+'/'+year;
        return formattedDate;
    }
    
