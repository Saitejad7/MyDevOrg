<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" 
                controller = "VMBrokerageApplicationListView" >
    
    <aura:handler name="init" value="{! this}" action="{! c.init }"/>
    
    <aura:attribute name="appid" type="String" />
    <aura:attribute name="vcmId" type="String" />
    <aura:attribute name="Spinner" type="boolean" default="false"/>
    
    <!-- Attribute for KYC_SoftFail_MSR_Pending Verify Screen Info Applications-->
    
    <aura:attribute name="isKYCSoftFailMSRPending" type="Boolean" default="false"/>    
    <aura:attribute name="verifyData" type="String" />
    <aura:attribute name="investorProfileandRisk" type="String" />
    <aura:attribute name="bdPepInfo" type="String" />
    <aura:attribute name="empStatus" type="String"/>
    <aura:attribute name="jointEmpStatus" type="String"/>
    <aura:attribute name="financialAnnualIncome" type="String"/>
    <aura:attribute name="financialNetWorth" type="String"/>
    <aura:attribute name="financialAnnualIncomeJoint" type="String"/>
    <aura:attribute name="financialNetWorthJoint" type="String"/>
    <aura:attribute name="includesIra" type="String"/>
    <aura:attribute name="beneficiariesData" type="sObject"/>
    <aura:attribute name="contingentData" type="sObject"/>
    <aura:attribute name="showError" type="Boolean" default="false"/>
    <aura:attribute name="diffResCode" type="String"/>
    <aura:attribute name="accountType" type="String"/>
    
    
    <aura:if isTrue="{!v.Spinner}">
        <div aura:id="spinnerId" class="slds-spinner_container">
            <div class="slds-spinner--brand  slds-spinner slds-spinner--medium slds-is-relative" role="alert">
                <span class="slds-assistive-text">Loading</span>
                <div class="slds-spinner__dot-a"></div>
                <div class="slds-spinner__dot-b"></div>
            </div>
        </div>
    </aura:if>    
    <div class="slds-modal__content slds-p-around_medium slds-scrollable" id="modal-content-id-1">                        
        <div class="c-container scrollable"> 
            <div class="slds-is-relative" style="">                
                        <b><h2 style="color: #4f799f!important;" id="modal-heading-01" class="slds-border_bottom slds-text-heading_medium slds-hyphenate">
                            VCM Investment Account Information</h2></b>
                        <br/>                                
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Registration</b></h2>
                            </div>
                            <aura:if isTrue="{!v.accountType == 'JTWROS'}">
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>Joint Tenants with Rights of Survivorship</span>
                                </div>
                                <aura:set attribute="else">
                                    <div class="slds-col slds-size_1-of-4 labelValue">
                                        <span>{!v.accountType}</span>
                                    </div>
                                </aura:set>
                            </aura:if>
                        </div>
                        <br/>
                        <b><h2 style="color: #4f799f!important;" class="slds-border_bottom slds-text-heading_medium slds-hyphenate">
                            Personal Information-Primary</h2></b>
                        <br/>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Name</b></h2>
                            </div>
                            <aura:if isTrue="{!not(empty(v.verifyData.result.personalInfo.primary.prefix))}">
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>
                                        {!v.verifyData.result.personalInfo.primary.prefix + ' ' +
                                        v.verifyData.result.personalInfo.primary.firstName + ' ' +
                                        v.verifyData.result.personalInfo.primary.lastName}
                                    </span>
                                </div>
                                <aura:set attribute="else">
                                    <div class="slds-col slds-size_1-of-4 labelValue">
                                        <span>
                                            {!v.verifyData.result.personalInfo.primary.firstName + ' ' +
                                            v.verifyData.result.personalInfo.primary.lastName}
                                        </span>
                                    </div>
                                </aura:set>
                            </aura:if>
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Date of Birth</b></h2>
                            </div>
                            <div class="slds-col slds-size_1-of-4 labelValue">
                                <span>{!v.verifyData.result.personalInfo.primary.maskedDob}</span>
                            </div>                                    
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Gender</b></h2>
                            </div>
                            <div class="slds-col slds-size_1-of-4 labelValue">
                                <span>{!v.verifyData.result.personalInfo.primary.gender}</span>
                            </div>                                    
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Marital Status</b></h2>
                            </div>
                            <div class="slds-col slds-size_1-of-4 labelValue">
                                <span>{!v.verifyData.result.personalInfo.primary.maritalStatus}</span>
                            </div>                                    
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Citizenship</b></h2>
                            </div>
                            <div class="slds-col slds-size_1-of-4 labelValue">
                                <span>{!v.verifyData.result.personalInfo.primary.citizenship}</span>
                            </div>                                    
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Social Security Number</b></h2>
                            </div>
                            <div class="slds-col slds-size_1-of-4 labelValue">
                                <span>{!v.verifyData.result.personalInfo.primary.maskedSSN}</span>
                            </div>                                    
                        </div>
                        <br/>
                        <b><h2 style="color: #4f799f!important;"  id="modal-heading-01" class="slds-border_bottom slds-text-heading_medium slds-hyphenate">
                            Contact Information</h2></b>
                        <br/>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_6-of-12">
                                <h2 class="labelKey"><b>Mailing Address</b></h2>
                                <aura:if isTrue="{!not(empty(v.verifyData.result.address.primary.mailingAddress.addressLine1))}">                                        
                                    <h2 class="labelValue">
                                        {!v.verifyData.result.address.primary.mailingAddress.addressLine2 + ',' +
                                        v.verifyData.result.address.primary.mailingAddress.addressLine1 + ',' +
                                        v.verifyData.result.address.primary.mailingAddress.city + ',' +
                                        v.verifyData.result.address.primary.mailingAddress.state + '-' +
                                        v.verifyData.result.address.primary.mailingAddress.zipCode}
                                    </h2>
                                    <aura:set attribute="else">
                                        <h2 class="labelValue">
                                            {!v.verifyData.result.address.primary.mailingAddress.addressLine2 + ',' +
                                            v.verifyData.result.address.primary.mailingAddress.city + ',' +
                                            v.verifyData.result.address.primary.mailingAddress.state + '-' +
                                            v.verifyData.result.address.primary.mailingAddress.zipCode}
                                        </h2>
                                    </aura:set>
                                </aura:if>
                            </div>                                   
                        </div>
                        <br/>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_6-of-12">
                                <h2 class="labelKey"><b>Physical Address</b></h2>                                        
                                <aura:if isTrue="{!not(empty(v.verifyData.result.address.primary.physicalAddress.addressLine1))}">                                        
                                    <h2 class="labelValue">
                                        {!v.verifyData.result.address.primary.physicalAddress.addressLine2 + ',' +
                                        v.verifyData.result.address.primary.physicalAddress.addressLine1 + ',' +
                                        v.verifyData.result.address.primary.physicalAddress.city + ',' +
                                        v.verifyData.result.address.primary.physicalAddress.state + '-' +
                                        v.verifyData.result.address.primary.physicalAddress.zipCode}
                                    </h2>
                                    <aura:set attribute="else">
                                        <h2 class="labelValue">
                                            {!v.verifyData.result.address.primary.physicalAddress.addressLine2 + ',' +
                                            v.verifyData.result.address.primary.physicalAddress.city + ',' +
                                            v.verifyData.result.address.primary.physicalAddress.state + '-' +
                                            v.verifyData.result.address.primary.physicalAddress.zipCode}
                                        </h2>
                                    </aura:set>
                                </aura:if>
                            </div>                                   
                        </div>
                        <br/>
                        <b><h2 style="color: #4f799f!important;"  id="modal-heading-01" class="slds-border_bottom slds-text-heading_medium slds-hyphenate">
                            Phone Information</h2></b>
                        <br/>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Primary Phone</b></h2>
                            </div>
                            <aura:if isTrue="{!not(empty(v.verifyData.result.contactInfo.primary.phoneInfo.primary.number))}">
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>{!v.verifyData.result.contactInfo.primary.phoneInfo.primary.countryCode + ' ' +
                                        v.verifyData.result.contactInfo.primary.phoneInfo.primary.number}</span>
                                </div>
                                <aura:set attribute="else">
                                    <b>-</b>
                                </aura:set>
                            </aura:if>
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Secondary Phone</b></h2>
                            </div>
                            <aura:if isTrue="{!not(empty(v.verifyData.result.contactInfo.primary.phoneInfo.secondary.number))}">
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>{!v.verifyData.result.contactInfo.primary.phoneInfo.secondary.countryCode + ' ' +
                                        v.verifyData.result.contactInfo.primary.phoneInfo.secondary.number}</span>
                                </div>
                                <aura:set attribute="else">
                                    <b>-</b> 
                                </aura:set>
                            </aura:if>                                                                      
                        </div>
                        <br/>
                        <b><h2 style="color: #4f799f!important;"  id="modal-heading-01" class="slds-border_bottom slds-text-heading_medium slds-hyphenate">
                            Email Information</h2></b>
                        <br/>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Primary Email</b></h2>
                            </div>
                            <div class="slds-col slds-size_1-of-4 labelValue">
                                <span>{!v.verifyData.result.contactInfo.primary.emailAddresses.maskedPrimaryEmailAddress}</span>
                            </div>                                                                                                          
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Secondary Email</b></h2>
                            </div>
                            <aura:if isTrue="{!not(empty(v.verifyData.result.contactInfo.primary.emailAddresses.secondaryEmailAddress))}">
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>{!v.verifyData.result.contactInfo.primary.emailAddresses.maskedSecondaryEmailAddress}</span>
                                </div>
                                <aura:set attribute="else">
                                    <b>-</b>
                                </aura:set>
                            </aura:if>                                                                     
                        </div>
                        <br/>
                        <b><h2 style="color: #4f799f!important;"  id="modal-heading-01" class="slds-border_bottom slds-text-heading_medium slds-hyphenate">
                            Employment Information-Primary</h2></b>
                        <br/>
                        <aura:if isTrue="{!((v.empStatus == 'Student') || (v.empStatus == 'Retired') || 
                                         (v.empStatus == 'Unemployed') || (v.empStatus == 'Homemaker')) }">
                            <div class="slds-grid slds-wrap">
                                <div class="slds-col slds-size_1-of-4 labelKey">
                                    <h2><b>Employment Status</b></h2>
                                </div>
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>{!v.verifyData.result.employmentInfo.primary.status}</span>
                                </div>                                    
                            </div>
                            <div class="slds-grid slds-wrap">
                                <div class="slds-col slds-size_1-of-4 labelKey">
                                    <h2><b>Primary Source of Income</b></h2>
                                </div>
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>{!v.verifyData.result.employmentInfo.primary.primarySource}</span>
                                </div>                                    
                            </div>
                            <aura:set attribute="else">
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-col slds-size_1-of-4 labelKey">
                                        <h2><b>Employment Status</b></h2>
                                    </div>
                                    <div class="slds-col slds-size_1-of-4 labelValue">
                                        <span>{!v.verifyData.result.employmentInfo.primary.status}</span>
                                    </div>                                    
                                </div>
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-col slds-size_1-of-4 labelKey">
                                        <h2><b>Industry</b></h2>
                                    </div>
                                    <div class="slds-col slds-size_1-of-4 labelValue">
                                        <span>{!v.verifyData.result.employmentInfo.primary.industry}</span>
                                    </div>                                    
                                </div>                                        
                                <aura:if isTrue="{!not(empty(v.verifyData.result.employmentInfo.primary.occupation))}">
                                    <div class="slds-grid slds-wrap">
                                        <div class="slds-col slds-size_1-of-4 labelKey">
                                            <h2><b>Occupation</b></h2>
                                        </div>
                                        <div class="slds-col slds-size_1-of-4 labelValue">
                                            <span>{!v.verifyData.result.employmentInfo.primary.occupation}</span>
                                        </div>                                    
                                    </div>                                             
                                </aura:if>
                                <aura:if isTrue="{!not(empty(v.verifyData.result.employmentInfo.primary.employerName))}">
                                    <div class="slds-grid slds-wrap">
                                        <div class="slds-col slds-size_1-of-4 labelKey">
                                            <h2><b>Employer's Name</b></h2>
                                        </div>
                                        <div class="slds-col slds-size_1-of-4 labelValue">
                                            <span>{!v.verifyData.result.employmentInfo.primary.employerName}</span>
                                        </div>                                    
                                    </div> 
                                </aura:if>                                        
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-col slds-size_1-of-4 labelKey">
                                        <h2><b>Employer's Address</b></h2>
                                    </div>
                                    <aura:if isTrue="{!not(empty(v.verifyData.result.employmentInfo.primary.address.addressLine2))}">
                                        <div class="slds-col slds-size_1-of-4 labelValue">
                                            <span>
                                                {!v.verifyData.result.employmentInfo.primary.address.addressLine2 + ',' +
                                                v.verifyData.result.employmentInfo.primary.address.city + ',' +
                                                v.verifyData.result.employmentInfo.primary.address.state + '-' +
                                                v.verifyData.result.employmentInfo.primary.address.zipCode}
                                            </span>
                                        </div>
                                        <aura:set attribute="else">
                                            <b>-</b>
                                        </aura:set>
                                    </aura:if>                                            
                                </div>                                                                                 
                            </aura:set>
                        </aura:if>
                        <br/>
                        <b><h2 style="color: #4f799f!important;"  id="modal-heading-01" class="slds-border_bottom slds-text-heading_medium slds-hyphenate">
                            Financial Information-Primary</h2></b>
                        <br/>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Annual Income (USD)</b></h2>
                            </div>
                            <div class="slds-col slds-size_1-of-4 labelValue">                                        
                                <span>{!v.financialAnnualIncome}</span>
                            </div>                                    
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Tax Bracket</b></h2>
                            </div>
                            <div class="slds-col slds-size_1-of-4 labelValue">
                                <span>{!v.verifyData.result.financialInfo.primary.taxBracket}%</span>
                            </div>                                    
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Net Worth (USD)</b></h2>
                            </div>
                            <div class="slds-col slds-size_1-of-4 labelValue">
                                <span>{!v.financialNetWorth}</span>
                            </div>                                    
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Tax Filing Status</b></h2>
                            </div>
                            <aura:if isTrue="{!not(empty(v.verifyData.result.financialInfo.primary.taxFilingStatus))}">
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>{!v.verifyData.result.financialInfo.primary.taxFilingStatus}</span>
                                </div>
                                <aura:set attribute="else">
                                    <b>-</b> 
                                </aura:set>
                            </aura:if>
                        </div>
                        <br/>
                        <b><h2 style="color: #4f799f!important;"  id="modal-heading-01" class="slds-border_bottom slds-text-heading_medium slds-hyphenate">
                            Military Information</h2></b>
                        <br/>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Military Status</b></h2>
                            </div>
                            <aura:if isTrue="{!not(empty(v.verifyData.result.militaryInfo.primary.militaryStatus))}">
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>{!v.verifyData.result.militaryInfo.primary.militaryStatus}</span>
                                </div>
                                <aura:set attribute="else">
                                    <b>-</b>
                                </aura:set>
                            </aura:if>
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Branch of Service</b></h2>
                            </div>
                            <aura:if isTrue="{!not(empty(v.verifyData.result.militaryInfo.primary.branch))}">
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>{!v.verifyData.result.militaryInfo.primary.branch}</span>
                                </div> 
                                <aura:set attribute="else">
                                    <b>-</b>
                                </aura:set>
                            </aura:if>
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Rank</b></h2>
                            </div>
                            <aura:if isTrue="{!not(empty(v.verifyData.result.militaryInfo.primary.rank))}">
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>{!v.verifyData.result.militaryInfo.primary.rank}</span>
                                </div>  
                                <aura:set attribute="else">
                                    <b>-</b>
                                </aura:set>
                            </aura:if>
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Dates of Service</b></h2>
                            </div>
                            <aura:if isTrue="{!and(not(empty(v.verifyData.result.militaryInfo.primary.startDate)), not(empty(v.verifyData.result.militaryInfo.primary.endDate)))}">
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>{!v.verifyData.result.militaryInfo.primary.startDate + '-' + v.verifyData.result.militaryInfo.primary.endDate}</span>
                                </div>
                                <aura:set attribute="else">
                                    <b>-</b>
                                </aura:set>
                            </aura:if>
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-4 labelKey">
                                <h2><b>Commission Source</b></h2>
                            </div>
                            <aura:if isTrue="{!not(empty(v.verifyData.result.militaryInfo.primary.commissionSource))}">
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>{!v.verifyData.result.militaryInfo.primary.commissionSource}</span>
                                </div>    
                                <aura:set attribute="else">
                                    <b>-</b>
                                </aura:set>
                            </aura:if>
                        </div>
                        <br/>
                        <aura:if isTrue="{!v.accountType == 'JTWROS'}">
                            <b><h2 style="color: #4f799f!important;"  id="modal-heading-01" class="slds-border_bottom slds-text-heading_medium slds-hyphenate">
                                Personal Information - Joint Owner</h2></b>
                            <br/>
                            <div class="slds-grid slds-wrap">
                                <div class="slds-col slds-size_1-of-4 labelKey">
                                    <h2><b>Relationship to Account Holder</b></h2>
                                </div>
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>{!v.verifyData.result.personalInfo.joint.relationship}</span>
                                </div>                                    
                            </div>
                            <div class="slds-grid slds-wrap">
                                <div class="slds-col slds-size_1-of-4 labelKey">
                                    <h2><b>Name</b></h2>
                                </div>
                                <aura:if isTrue="{!not(empty(v.verifyData.result.personalInfo.joint.prefix))}">
                                    <div class="slds-col slds-size_1-of-4 labelValue">
                                        <span>
                                            {!v.verifyData.result.personalInfo.joint.prefix + ' ' +
                                            v.verifyData.result.personalInfo.joint.firstName + ' ' +
                                            v.verifyData.result.personalInfo.joint.lastName}
                                        </span>
                                    </div>
                                    <aura:set attribute="else">
                                        <div class="slds-col slds-size_1-of-4 labelValue">
                                            <span>
                                                {!v.verifyData.result.personalInfo.joint.firstName + ' ' +
                                                v.verifyData.result.personalInfo.joint.lastName}
                                            </span>
                                        </div>
                                    </aura:set>
                                </aura:if>
                            </div>
                            <div class="slds-grid slds-wrap">
                                <div class="slds-col slds-size_1-of-4 labelKey">
                                    <h2><b>Date of Birth</b></h2>
                                </div>
                                <div class="slds-col slds-size_1-of-4 labelValue">
                                    <span>{!v.verifyData.result.personalInfo.joint.dob}</span>
                                </div>                                    
                            </div>
                            <div class="slds-grid slds-wrap">
                                <div class="slds-col slds-size_1-of-4 labelKey">
                                    <h2><b>Gender</b></h2>
                                </div>
